    <hudson.tasks.Shell>
      <command>#!/bin/bash

echo &quot;$run_standalone&quot;
if [[ &quot;$run_standalone&quot; != &quot;true&quot; ]]; then
  if [[ ! -f config/new_config.properties ]]; then
    echo &quot;Artifact new_config.properties does not exist.&quot;
    exit 1
  else
      echo &quot;Artifact new_config.properties does exist.&quot;
      cat config/new_config.properties
      cp config/new_config.properties config.properties
  fi
else
	echo &quot;Running as standalone job&quot;
fi

</command>
